export function newMaintenanceRequest({ userId, equipmentId, costUSD, selectedDate, type = 'corrective', notes = '' }) {
  return {
    userId,
    equipmentId,
    type,
    status: 'pending',
    costUSD,
    selectedDate,
    createdAt: new Date().toISOString(),
    notes,
    assignedTechnicianId: null
  };
}
